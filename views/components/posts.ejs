<div class="container">
    <div class="row">
        <div>
            <% posts.forEach(function (post) { %>
            <div>
                <h2>
                    <%- post.title %>
                </h2>
                <p><%- post.author.name %></p>
                <p><%- post.content %></p>
            </div>
            <% }) %>
        </div>
    </div>
</div>